package jrJava.twoDimenArray;

public class Practice1 {

	public static void main(String[] args) {
		
		int[][] a = new int[4][];
			
		a[0] = new int[]{1, 2, 3};
		a[1] = new int[]{11, 12, 13, 14};
		a[2] = new int[]{21, 22};
		a[3] = new int[]{31, 32, 33, 34, 35};
		
		System.out.println(a[0].length);
		System.out.println(a[3][4]);
	}

}
